<!DOCTYPE html>
<html lang="en">

<head>
  <link rel="stylesheet" href="/css/init.css">
  <link rel="stylesheet" href="/css/sidebar.css">
  <link rel="stylesheet" href="/css/header_footer.css">
  <link rel="stylesheet" href="/css/coordinador/calificaciones.css">
  <meta charset="UTF-8">
</head>

<body>

    <header>
        <div id="left-header">
            <a href="#"><img src="/images/logo.png" alt="logo"></a>
            <h1>Colegio Santa Marta - Representantes</h1>
        </div>
    
        <div id="right-header">
            <a href="/auth/logout">Salir</a>
        <div>
    </header>
    
    <nav id="sidebar">
       
         <a href="/representante" class="actual-link-sidebar">Inicio</a>
         <a href="/representante/consultar-notas">Consultar Notas</a>
         
    </nav>

  <main class="content-main">

    <div class="header-main">
        <a href="/representante/consultar-notas" class="nuevo">Regresar</a>
    </div>

    <div class="card-docente">

        <div class="header-inside">
            <h2>
                Bolet√≠n De Notas
            </h2>
        </div>

        <table>
            <tr>
                <th>Asignatura</th>
                <th>Lapso 1</th>
                <th>Lapso 2</th>
                <th>Lapso 3</th>
            </tr>

            <% data.boletin.asignaturas.forEach(asignatura => { %>
                <tr>
                  <td><%= asignatura.nombre %></td>
                  <td><%= asignatura.lapso1 %></td>
                  <td><%= asignatura.lapso2 %></td>
                  <td><%= asignatura.lapso3 %></td>
                </tr>
              <% }) %>
        </table>



  
    </div>

    <footer>
      <p>&copy; 2024. Colegio Santa Marta. Todos los derechos reservados.</p>
    </footer>

    <!-- ERRORES -->
   

</body>

</html>